<% provide(:title, t("admin.orders.index.title")) %>
<h1><%= t "admin.orders.index.title" %></h1>
<table class="table">
  <thead>
    <tr>
      <th scope="col"><%= t "admin.orders.index.id" %></th>
      <th scope="col"><%= t "admin.orders.index.customer" %></th>
      <th scope="col"><%= t "admin.orders.index.status" %></th>
